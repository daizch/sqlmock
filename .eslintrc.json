{
    "env": {
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 6
    },
    "extends": "airbnb-base/legacy",
    "rules": {
        "global-require": 0,
        "no-console": 0,
        "func-names": 0,
        "no-underscore-dangle": 0,
        "max-len": 0,
        "vars-on-top": 0,
        "no-use-before-define": 0,
        "no-empty": 0,
        "no-else-return": 0,
        "quote-props": 0,
        "object-curly-spacing": 0,
        "eqeqeq": 0,
        "spaced-comment": 0,
        "no-param-reassign": 0,
        "consistent-return": 0,
        "no-unused-expressions": ["error", { "allowShortCircuit": true, "allowTernary": true }],
        "indent": [ "error", 4, { "SwitchCase": 1}]
    }
}
